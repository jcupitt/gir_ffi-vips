# op = Vips::Operation::new "jpegload"
# op.methods
#
# => [:get_flags, :invalidate, :argument_isset, :argument_needsstring, :build,
# :get_argument_flags, :get_argument_priority, :get_argument_to_string,
# :get_description, :local_cb, :preclose, :print_dump, :print_name,
